{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "lib": ["DOM", "ESNext"],
    "module": "ESNext",
    "noEmit": true,
    "noImplicitAny": false,
    "paths": {
      "@/*": ["./*"]
    },
    "target": "ESNext",
    "types": [
      "bun-types",
      "element-plus/global",
      "vite/client",
      "vite-plugin-pwa/client"
    ]
  }
}
