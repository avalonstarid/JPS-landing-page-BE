{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-redis": "*",
        "aaronfrancis/fast-paginate": "^2.0",
        "akaunting/laravel-money": "^6.0",
        "knuckleswtf/scribe": "^5.5",
        "laravel/framework": "^12.40",
        "laravel/octane": "^2.13",
        "laravel/pulse": "^1.4",
        "laravel/reverb": "^1.6",
        "laravel/sanctum": "^4.2",
        "laravel/telescope": "^5.15",
        "laravel/tinker": "^2.10.1",
        "league/flysystem-aws-s3-v3": "^3.30",
        "maatwebsite/excel": "^3.1",
        "me-shaon/laravel-request-analytics": "^1.8",
        "opcodesio/log-viewer": "^3.21",
        "ralphjsmit/laravel-seo": "^1.7",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-backup": "^9.3",
        "spatie/laravel-image-optimizer": "^1.8",
        "spatie/laravel-medialibrary": "^11.17",
        "spatie/laravel-one-time-passwords": "^1.0",
        "spatie/laravel-permission": "^6.23",
        "spatie/laravel-query-builder": "^6.3",
        "spatie/laravel-sitemap": "^7.3",
        "spatie/laravel-sluggable": "^3.7",
        "spatie/laravel-translatable": "^6.12",
        "whichbrowser/parser": "^2.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel-lang/lang": "^15.26",
        "laravel/boost": "^1.8",
        "laravel/pail": "^1.2",
        "laravel/sail": "^1.47",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.7",
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan lang:update"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"npm run dev\" --names='server,queue,vite'"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
